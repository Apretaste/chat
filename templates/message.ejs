<div class="row">
    <div class="col s12 center">
        <h1 class="black-text"><%= header %></h1>
        <i class="material-icons large"><%= icon %></i>
        <p><%= text %></p>
        <a class="waves-effect waves-light btn piropazo-color" href="#!"
           onclick="apretaste.send({'command':'<%= button['href'] %>', data: {<%= button['data'] ?? '' %>}})">
            <%= button['caption'] %>
        </a>
    </div>

    <div class="fixed-action-btn">
        <a class="btn-floating btn-large waves-effect waves-light green" href="#!"
           onclick="apretaste.back()">
            <i class="material-icons">arrow_back</i>
        </a>
    </div>
</div>