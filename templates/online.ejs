<div class="row">
	<div class="col s12">
		<ul class="tabs tabs-fixed-width">
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'CHAT OPEN'})">Abiertos</a></li>
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'CHAT SEARCH'})">Buscar</a></li>
			<li class="tab"><a href="#" class="active">Online</a></li>
		</ul>
	</div>
</div>

<div class="row">
	<% users.forEach(function(item){ %>
		<div class="col s4 m2">
			<div class="card center-align">
				<div class="content" onclick="profile('<%= item.username %>')">
					<div class="person-avatar circle" face="<%= item.avatar %>" color="<%= item.avatarColor %>" size="40" online="1"></div>
					<div class="<%= item.gender %>">@<%= short(item.username) %></div>
				</div>
				<div class="action grey lighten-2 green-text">
					<a href="#!" onclick="chat('<%= item.id %>')"><i class="material-icons">chat</i></a>
				</div>
			</div>
		</div>
	<% }) %>
</div>